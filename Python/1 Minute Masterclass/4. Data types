# String (text)
name = "Python"
print("Name:", name)
print("Type of name:", type(name))

# Integer (whole number)
age = 10
print("Age:", age)
print("Type of age:", type(age))

# Float (decimal number)
version = 3.11
print("Version:", version)
print("Type of version:", type(version))
